{
  "name": "react-smart-calendar",
  "version": "1.0.10",
  "description": "A reusable calendar component for React with month and year navigation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/style.css",
    "README.md"
  ],
  "keywords": [
    "react",
    "calendar",
    "date-picker",
    "date",
    "component",
    "typescript",
    "smart-calendar",
    "react-smart-calendar"
  ],
  "author": "Nitin Navik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NAVIKNITIN/react-calendar.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/fix-types.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
